<!DOCTYPE html>
<html style="font-size: 16px; lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="keywords" content="Climb the mountain not to plant your flag">
    <meta name="description" content="">
    <meta name="generator" content="Nicepage 5.0.18, nicepage.com">
    <meta name="theme-color" content="#4a87ec">
    <meta property="og:type" content="website">
    <title>@ViewData["Title"] - Karpatus</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" media="screen">
    <link rel="stylesheet" href="~/css/style.css" media="screen">
    <link rel="stylesheet" href="~/css/nicepage.css" media="screen">
    <link rel="stylesheet" href="~/css/Blog-Template.css" media="screen">
    <link id="u-theme-google-font" rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i|Open+Sans:300,300i,400,400i,500,500i,600,600i,700,700i,800,800i">
    <link id="u-page-google-font" rel="stylesheet" href="https://fonts.googleapis.com/css?family=PT+Sans:400,400i,700,700i">
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script class="u-script" type="text/javascript" src="~/lib/jquery/jquery-1.9.1.min.js" defer=""></script>
    <script class="u-script" type="text/javascript" src="~/lib/jquery/nicepage.js" defer=""></script>

    @await RenderSectionAsync("Scripts", required: false)
</head>

<body>
    <header>
        <nav class="navbar navbar-expand-sm bg-dark navbar-dark fixed-top">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"><img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQhDdtdzz5aiGCDheEVrrqo8n1CHFvSVQkAng&usqp=CAU" class="rounded-circle" alt="Cinque Terre" width="50" height="50">Karpatus</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#thetarget">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <nav class="u-menu u-menu-dropdown u-offcanvas u-menu-1">
                    <div class="menu-collapse" style="font-size: 1rem; letter-spacing: 0px;">
                        <a class="u-button-style u-custom-left-right-menu-spacing u-custom-padding-bottom u-custom-top-bottom-menu-spacing u-nav-link u-text-active-palette-1-base u-text-hover-palette-2-base" href="#">
                            <svg class="u-svg-link" viewBox="0 0 24 24"><use xlink:href="#menu-hamburger"></use></svg>
                            <svg class="u-svg-content" version="1.1" id="menu-hamburger" viewBox="0 0 16 16" x="0px" y="0px" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns="http://www.w3.org/2000/svg">
                                <g>
                                    <rect y="1" width="16" height="2"></rect>
                                    <rect y="7" width="16" height="2"></rect>
                                    <rect y="13" width="16" height="2"></rect>
                                </g>
                            </svg>
                        </a>
                    </div>
                    <div class="u-nav-container">
                        <ul class="u-nav u-unstyled u-nav-1">
                            <li class="u-nav-item">
                                <a class="nav-link text-light" asp-area="" asp-controller="Event" asp-action="Event">Event</a>
                            </li>
                            <li class="u-nav-item">
                                <a class="nav-link text-light" asp-area="" asp-controller="Place" asp-action="Place">Place</a>
                            </li>
                            <li class="u-nav-item">
                                <a class="nav-link text-light" asp-area="" asp-controller="Cardus" asp-action="Cardus">Cardus</a>
                            </li>
                            <li class="u-nav-item">
                                <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Info">Information</a>
                            </li>
                        </ul>
                    </div>
                    <div class="u-nav-container-collapse">
                        <div class="u-black u-container-style u-inner-container-layout u-opacity u-opacity-95 u-sidenav">
                            <div class="u-inner-container-layout u-sidenav-overflow">
                                <div class="u-menu-close"></div>
                                <ul class="u-align-center u-nav u-popupmenu-items u-unstyled u-nav-2">
                                    
                                    <li class="u-nav-item">
                                        <a class="nav-link text-light" asp-area="" asp-controller="Event" asp-action="Event">Event</a>
                                    </li>
                                    <li class="u-nav-item">
                                        <a class="nav-link text-light" asp-area="" asp-controller="Place" asp-action="Place">Place</a>
                                    </li>
                                    <li class="u-nav-item">
                                        <a class="nav-link text-light" asp-area="" asp-controller="Cardus" asp-action="Cardus">Cardus</a>
                                    </li>
                                    <li class="u-nav-item">
                                        <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Info">Information</a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <div class="u-black u-menu-overlay u-opacity u-opacity-70"></div>
                    </div>
                </nav>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="u-align-center u-clearfix u-footer u-grey-80 u-footer" id="sec-9440">
        <div class="u-clearfix u-sheet u-sheet-1">
            <p class="u-small-text u-text u-text-variant u-text-1" data-lang-en="Sample text. Click to select the text box. Click again or double click to start editing the text.">&copy; 2022 - Karpatus - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a></p>
        </div>
    </footer>


</body>
</html>
